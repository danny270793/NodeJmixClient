{
    "name": "@danny270793/jmix-client",
    "version": "1.0.2",
    "description": "jmix rest api client",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build/**/*"
    ],
    "scripts": {
        "install:force": "rm -r -f node_modules && npm install -f",
        "format": "pretty-quick",
        "lint": "eslint . --ext .ts",
        "documentate": "typedoc --out ./public/docs src",
        "build": "tsc",
        "start": "node build/index.js",
        "start:watch": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"ts-node src/index.ts\"",
        "test": "jest --config jestconfig.json",
        "test:watch": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run test\""
    },
    "keywords": [
        "jmix",
        "rest-client"
    ],
    "author": {
        "name": "Danny Vaca",
        "email": "danny270793@gmail.com",
        "url": "https://danny270793.github.io/#/"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/danny-vaca/libraries/node/jmix-client"
    },
    "license": "UNLICENSED",
    "dependencies": {
        "axios": "^0.27.2",
        "ts-node": "^10.8.1",
        "winston": "^3.7.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/jest": "^28.1.1",
        "@types/node": "^17.0.41",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "dotenv": "^16.0.1",
        "eslint": "^8.17.0",
        "jest": "^28.1.1",
        "jest-html-reporters": "^3.0.9",
        "jest-junit": "^13.2.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "ts-jest": "^28.0.4",
        "typedoc": "^0.22.17"
    }
}
