{
    "name": "@danny270793/jmix-client",
    "version": "1.0.0",
    "description": "jmix rest api client",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "files": [
        "build/**/*"
    ],
    "scripts": {
        "install:force": "rm -r -f node_modules && npm install -f",
        "format": "prettier ./ --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "build": "tsc",
        "build:force": "rm -r -f ./build && tsc",
        "start": "node ./build/index.js",
        "start:watch": "ts-node-dev --respawn ./src/index.ts",
        "test": "ts-node-dev ./tests/index.test.ts",
        "test:watch": "ts-node-dev --respawn ./tests/index.test.ts",
        "documentate": "typedoc --out ./public/docs ./src"
    },
    "keywords": [
        "jmix",
        "rest-client"
    ],
    "author": {
        "name": "Danny Vaca",
        "email": "danny270793@gmail.com",
        "url": "https://danny270793.github.io/#/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/danny-vaca/libraries/node/jmix-client"
    },
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/node": "^22.4.2",
        "eslint": "^9.9.0",
        "globals": "^15.9.0",
        "logform": "^2.6.1",
        "prettier": "^3.3.3",
        "ts-node-dev": "^2.0.0",
        "typedoc": "^0.26.6",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.2.0",
        "winston": "^3.14.2"
    }
}
